<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/feed_bg"
    android:orientation="vertical"
    >

    <LinearLayout
        android:layout_marginLeft="@dimen/feed_item_margin"
        android:layout_marginRight="@dimen/feed_item_margin"
        android:layout_marginTop="@dimen/feed_item_margin"
        android:background="@drawable/bg_parent_rounded_corner"
        android:orientation="vertical"
        android:paddingBottom="@dimen/feed_item_padding_top_bottom"
        android:paddingTop="@dimen/feed_item_padding_top_bottom"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
    <RelativeLayout
        android:layout_margin="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:text="Awb Number :"
            android:id="@+id/awbno"
            android:textSize="16dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:paddingLeft="35dp"
          android:layout_toRightOf="@+id/awbno"
            android:id="@+id/awb_number"
            android:layout_width="wrap_content"
            android:text="Awb Number :"
            android:textSize="16dp"
            android:layout_height="wrap_content"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_margin="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:text="Phone number :"
            android:textSize="16dp"
            android:id="@+id/phoneno"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:layout_toRightOf="@+id/phoneno"
            android:id="@+id/phoneNo"
            android:paddingLeft="29dp"
            android:layout_width="wrap_content"
            android:text="Awb Number :"
            android:textSize="16dp"
            android:layout_height="wrap_content"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_margin="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:text="Delivery Date :"
            android:id="@+id/deliverydate"
            android:textSize="16dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:layout_toRightOf="@+id/deliverydate"
            android:paddingLeft="38dp"
            android:id="@+id/statusDate"
            android:layout_width="wrap_content"
            android:text="Awb Number :"
            android:textSize="16dp"
            android:layout_height="wrap_content"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_margin="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:text="Recived by :"
            android:id="@+id/reciveby"
            android:textSize="16dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:layout_toRightOf="@+id/reciveby"
            android:paddingLeft="54dp"
            android:id="@+id/revdBy"
            android:layout_width="wrap_content"
            android:text="Awb Number :"
            android:textSize="16dp"
            android:layout_height="wrap_content"
            />
    </RelativeLayout>

    <RelativeLayout
        android:layout_margin="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:text="DeliveryEmpCode :"
            android:id="@+id/deliveryempcode"
            android:textSize="16dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:layout_toRightOf="@+id/deliveryempcode"
            android:paddingLeft="8dp"
            android:id="@+id/deliveryEmpCode"
            android:layout_width="wrap_content"
            android:text="Awb Number :"
            android:textSize="16dp"
            android:layout_height="wrap_content"
            />
    </RelativeLayout>

